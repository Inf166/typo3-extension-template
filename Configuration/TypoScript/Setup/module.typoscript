    settings < plugin.tx_templatepackage.settings

    view {
      layoutRootPaths {
        5 = {$view.pages.partialRootPath}
        10 = {$view.backend.layoutRootPath}
      }

      partialRootPaths {
        5 = {$view.pages.partialRootPath}
        10 = {$view.backend.partialRootPath}
      }

      templateRootPaths {
