module {
  tx_templatetypo3extension {

    settings < plugin.tx_templatetypo3extension.settings

    view {
      layoutRootPaths {
        5 = {$view.pages.partialRootPath}
        10 = {$view.backend.layoutRootPath}
      }

      partialRootPaths {
        5 = {$view.pages.partialRootPath}
        10 = {$view.backend.partialRootPath}
      }

      templateRootPaths {
        5 = {$view.pages.partialRootPath}
        10 = {$view.backend.templateRootPath}
      }
    }

  }
}
